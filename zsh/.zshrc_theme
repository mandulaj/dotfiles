# Theme

case "$(hostname)" in
    "mercury")
        export AGNOSTER_DIR_BG="250;5"
        export AGNOSTER_CONTEXT_FG="250;5"
        ;;
    "mars")
        export AGNOSTER_DIR_BG="208;5"
        export AGNOSTER_CONTEXT_FG="208;5"
        ;;
    "venus")
        export AGNOSTER_DIR_BG="yellow"
        export AGNOSTER_CONTEXT_FG="yellow"
        ;;
    "jupiter-pbl")
        export AGNOSTER_DIR_BG="197;5"
        export AGNOSTER_CONTEXT_FG="197;5"
        ;;
    "neptune")
        export AGNOSTER_DIR_BG="99;5"
        export AGNOSTER_CONTEXT_FG="99;5"
        ;;
    "sentinel")
        export AGNOSTER_DIR_BG="154;5"
        export AGNOSTER_CONTEXT_FG="154;5"
	;;
    "trinity")
        export AGNOSTER_DIR_BG="84;5"
        export AGNOSTER_CONTEXT_FG="84;5"
        ;;
    "pve-ubuntu")
        export AGNOSTER_DIR_BG="124;5"
        export AGNOSTER_CONTEXT_FG="202;5"
        ;;
    "laplace")
        export AGNOSTER_DIR_BG="91;5"
        export AGNOSTER_CONTEXT_FG="129;5"
        ;;
    "kvok")
        export AGNOSTER_DIR_BG="136;5"
        export AGNOSTER_CONTEXT_FG="136;5"
        ;;
    *)
        export AGNOSTER_DIR_BG="cyan"
        ;;
esac
